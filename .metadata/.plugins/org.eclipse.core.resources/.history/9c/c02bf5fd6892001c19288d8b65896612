package Class12;

public class Horse {
	String name;
	String breed;
	int age;
	double weight; 
	
	void run() {
		System.out.println(name +" is running");
	}
	void eat() {
		System.out.println(name+ " is eating");
	}

	public static void main(String[] args) {
		Horse besh=new Horse();
		besh.name="spirit";
		besh.breed="stallion";
		besh.age=25;
		besh.weight=400;
		besh.run();
		

	}

}
