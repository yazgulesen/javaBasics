package com.syntax.class10;

public class Replit {

	public static void main(String[] args) {
		
		 char[] bc = { 's', 'a', 'y', 'b', 'n', 'c', 't', 'd', 'a', 'e', 'x' };
		 for(int i=0;i<bc.length;i++) {
			 System.out.println(bc[i]);
		 }
		 
		    System.out.println(bc[0]+bc[2]+bc[4]+bc[6]+bc[8]+bc[10]);
	}

}
