package com.syntaxjavaclass009;

public class LargestNumberArrayLoop {

	public static void main(String[] args) {
      
		
		int[]num= {23,45,65,77,54,1,900,79,34,2000};
		int largeN=num[0];
		for(int j:num) {
			if(j>largeN)
				largeN=j;
			
				
		}
		System.out.println("The largest num "+largeN);
	}

}
